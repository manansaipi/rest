{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": 0,
        "semi": [
            "error",
            "never"
        ],
        "max-len": [
            "error",
            {
                "code": 300
            }
        ],
        "spaced-comment": "off",
        "no-trailing-spaces": "off",
        "no-console": "off",
        "comma-dangle": "off",
        "no-multiple-empty-lines": "off"
    }
}
